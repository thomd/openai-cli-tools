#!/usr/bin/env bash

# this is a memory word game: show a list of ten random German words, then ask for repeating it

num=${1:-10}
echo -e "\n $(tput setaf 0)Repeat the following ${num} words in one line separated by whitespace:$(tput sgr 0)\n\n"
words=""
for (( i=1; i<=$num; i++ ))
do
  rnd=$(( $(awk '/^# WORDLIST$/{print NR}' "$0") + 2 + $RANDOM % $(wc -l < "$0") ))
  word="$(sed -n "${rnd}p" "$0" | awk '{print $2}')"
  echo -e "$(tput cuu 1)$(printf '  %-50s' "$word")"
  sleep 2
  words="${words} $word"
done
echo -e "$(tput cuu 1)$(printf '%-50s' " ")"

echo -e "$(tput cuu 2)"
read -p "> " input

echo -e "\n $(tput setaf 0)Correctly repeated words:$(tput sgr 0)\n"

TEXT="${words}\n${input}"
TASK="You will be provided with a list of whitespace separated German words. Then you will be provided with a second list of whitespace separated German words. Compare both lists. Treat misspelled words as matching. Print only the matching words as a list with dash bullets."
MODEL="gpt-3.5-turbo-16k"
BODY="{\"model\":\"${MODEL}\",\"messages\":[{\"role\":\"system\",\"content\":\"${TASK}\"},{\"role\":\"user\",\"content\":\"${TEXT}\"}]}"

success=$(curl https://api.openai.com/v1/chat/completions \
  -H "Content-Type: application/json" \
  -H "Authorization: Bearer $CHATGPT_API_KEY" \
  -d "${BODY}" \
  -s \
  | jq -r '.choices[].message.content' \
  | tee /dev/stderr \
  | wc -l)

echo -e "\n $(tput setaf 0)Your history:$(tput sgr 0)\n"
echo $success >> ~/.words
#cat ~/.words | uplot count
cat ~/.words | sort | uniq -c | awk '{print $2 " words: " $1}' | sort -nr

